et.itemsContainer[i].id === id) {\n                index = i;\n                console.log(index)\n            }\n            console.log(index)\n\n        }\n        budget.itemsContainer.splice(index, 1);\n    }\n\n\n    showBudget(budget) {\n        console.log(budget);\n    }\n\n    calculateMoney(arr, type) {\n        const arrayByType = arr.filter(function (item) {\n            return item.type === type;\n        });\n        return arrayByType.reduce((sum, item) => sum + item.value, 0);\n    }\n\n    calculateBudget2() {\n\n\n    }\n\n    calculateBudget() {\n        this.totalExpenses = calculateMoney(this.itemsContainer, \"exp\");\n        this.totalIncomes = calculateMoney(this.itemsContainer, \"inc\");\n        this.total = this.totalIncomes - this.totalExpenses;\n    }\n\n    getBudget() {\n        return {\n            total: this.total,\n            totalExpenses: this.totalExpenses,\n            totalIncomes: this.totalIncomes,\n        };\n    }\n\n    reset() {\n        this.expenses = [];\n        this.incomes = [];\n        this.totalExpenses = 0;\n        this.totalIncomes = 0;\n        this.total = 0;\n    }\n}\n\nexport default Budget;"},"lineCount":null}},"error":null,"hash":"53fb6bad304fc369b0bc6dae33476214","cacheData":{"env":{}}}