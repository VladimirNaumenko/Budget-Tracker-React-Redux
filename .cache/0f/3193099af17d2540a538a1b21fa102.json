app","generated":{"line":119,"column":4},"source":"src/app.js","original":{"line":71,"column":6}},{"generated":{"line":119,"column":7},"source":"src/app.js","original":{"line":71,"column":9}},{"generated":{"line":119,"column":10},"source":"src/app.js","original":{"line":71,"column":12}},{"name":"Application","generated":{"line":119,"column":14},"source":"src/app.js","original":{"line":71,"column":16}},{"generated":{"line":119,"column":25},"source":"src/app.js","original":{"line":71,"column":12}},{"generated":{"line":119,"column":27},"source":"src/app.js","original":{"line":71,"column":0}},{"name":"app","generated":{"line":120,"column":15},"source":"src/app.js","original":{"line":72,"column":15}}],"sources":{"src/app.js":"import Budget from \"./models/Budget\";\nimport View, {htmlElements} from \"./View\";\nimport Category from \"./models/Categories\";\n\n\n\nclass Application {\n    constructor() {\n        this.budget = new Budget();\n    }\n\n    init() {\n        View.displayBudget();\n        this.setupEventListeners();\n    }\n\n    setupEventListeners() {\n        htmlElements.submitBtn.addEventListener(\"click\", () => {\n            this.addItem();\n            console.log(this.budget)\n        });\n        document.querySelector(\"#btn-reset\").addEventListener(\"click\", () => {\n            View.reset();\n            this.budget.reset();\n        });\n        document.querySelector(\".header-budget\").addEventListener(\"click\", () => {\n            Category.renderCategory(this.budget.incomes, htmlElements.categoryChose.value);\n            console.log(this.budget);\n        });\n\n        document.querySelector(\".statistics\").addEventListener(\"click\", () => {\n            console.log(\"statistics\");\n            if (event.target.id === \"delete\") {\n                let myId = +event.target.parentElement.parentElement.getAttribute(\"data-id\");\n                let type = event.target.parentElement.parentElement.getAttribute(\"data-type\");\n                this.budget.myDeleteItem(this.budget, myId);\n                this.budget.calculateBudget();\n                View.displayBudget(this.budget);\n                (event.target.parentElement).parentElement.remove();\n            }\n        });\n    }\n\n    addItem() {\n        // get data from inputs\n        const inputData = View.getInputData();\n        const isValid = this.validateUserData(inputData);\n        console.log(View.getInputData());\n        if (isValid) {\n            const {type, category, description, value} = inputData;\n            console.log(inputData)\n            const item = this.budget.addItem(type, category, description, value);\n            View.addListItem(item, type, category);\n            View.clearInputs();\n            View.displayBudget({\n                totalExpenses: this.budget.totalExpenses,\n                totalIncomes: this.budget.totalIncomes,\n                total: this.budget.total\n            });\n        }\n    }\n\n    validateUserData(data) {\n        let reg = /[0-9]/g;\n        return !(data.description === \"\" || data.value === \"\"\n            || !(data.value.match(reg))\n        );\n    }\n}\n\nconst app = new Application();\nexport default app;"},"lineCount":null}},"error":null,"hash":"cb7ab771bb7fe7723ba1f1c23003e79d","cacheData":{"env":{}}}